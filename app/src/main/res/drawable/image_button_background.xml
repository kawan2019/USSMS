<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

            <padding android:top="1dp" android:right="1dp" android:bottom="5dp" android:left="1dp" />

            <gradient android:startColor="#25797979" android:useLevel="true"
                android:endColor="#CEFFFFFF"
                android:centerY="200"
                />
            <corners android:radius="12dp" />
        </shape>
    </item>

    <!-- Background -->
    <item>
        <shape>
            <solid android:color="#fefefe" />
            <corners android:radius="12dp" />
        </shape>
    </item>
</layer-list>