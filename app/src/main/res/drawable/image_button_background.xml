<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <padding android:top="1dp" android:right="1dp" android:bottom="5dp" android:left="1dp" />
            <stroke android:color="#30000000" android:width="3dp" />
            <corners android:radius="12dp" />
        </shape>
    </item>

    <!-- Background -->
    <item>
        <shape>
            <solid android:color="#fefefe" />
            <corners android:radius="12dp" />
        </shape>
    </item>
</layer-list>